                                    <div class="about-img-1">
                                        {#                                        <img src="assets/img/about-1.jpg" alt=""> #}
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="about-img">
                                    <div class="about-img-2">
                                        {#                                        <img src="assets/img/about-2.jpg" alt=""> #}
